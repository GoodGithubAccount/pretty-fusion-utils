{
  "generator": "retain_lines",
  "rules": [
    {
      "current": {
        "name": "path",
        "sources": {
          "test/": "test/types",
          "types": "src/types",
          "use-camera": "src/use-camera",
          "use-cleanup": "src/use-cleanup",
          "use-coroutine": "src/use-coroutine",
          "use-effect": "src/use-effect",
          "use-event-listener": "src/use-event-listener",
          "use-eventual": "src/use-eventual",
          "use-interval": "src/use-interval",
          "use-motion": "src/use-motion",
          "use-mouse": "src/use-mouse",
          "use-previous": "src/use-previous",
          "use-px": "src/use-px",
          "use-spring": "src/use-spring",
          "use-tagged": "src/use-tagged",
          "use-thread": "src/use-thread",
          "use-timer": "src/use-timer",
          "use-viewport": "src/use-viewport",
          "utils": "src/utils"
        }
      },
      "rule": "convert_require",
      "target": {
        "indexing_style": "property",
        "name": "roblox",
        "rojo_sourcemap": "./sourcemap.json"
      }
    },
    {
      "identifier": "__DEV__",
      "rule": "inject_global_value",
      "value": false
    },
    "compute_expression",
    "remove_unused_if_branch",
    "remove_unused_while",
    "filter_after_early_return",
    "remove_nil_declaration",
    "remove_empty_do",
    "remove_spaces"
  ]
}
